<pumastatectrl use_p_state_0_as_max_freq="true">
  <p_states>
   <!--  performance state 0: 
      for maximum frequency of 2,4 GHz: set VID to ca. 36 (when higher VID the device, e.g. laptop, may freeze)
      for maximum frequency of 2,3 GHz: set VID to ca. 38
      for maximum frequency of 2,2 GHz: set VID to ca. 40
      -->
   <pstate number="0" VID="38"></pstate>
   <!--  performance state 1: 
      for maximum frequency of 2,2 GHz: set VID to ca. 56 for 1100 MHz
      -->
   <pstate number="1" VID="56"></pstate>
   <!--  performance state 2: 
      -usually 1/4 of max frequency for "ZM" and "RM" series of Griffin
      -for other series than "ZM" and "RM" this Performance STATE is usually not used by the OS.
      -set VID to 64 for 1/4 of max frequency 
      -VIDs higher than 64 are usually not effective
      -->
   <pstate number="2" VID="64"></pstate>
  </p_states>
